import "../../src/lib/simple-csp-toString.eol";
import "../../src/lib/convert-cnf.eol";
import "../../src/lib/simplify-cnf.eol";

for( constr in Exp!Constraint.all) {

	constr.name.println();
	
	var orig	= constr.exp;
	var smp1 	= orig.simplify(); 
	var cnf 	= smp1.convertToCNF();
	var smp2 	= cnf.simplify(); 

	orig.asString().println("\t");
	smp1.asString().println("\t");
	cnf.asString().println("\t");
	smp2.asString().println("\t");
}